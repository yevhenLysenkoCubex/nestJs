{
   "scripts": {
      "prisma:dev:deploy": "prisma migrate deploy",
      "db:dev:rm": "docker compose rm dev-db -s -f -v",
      "db:dev:up": "docker compose up dev-db -d",
      "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up && sleep 1 && npm run prisma:dev:deploy",
      "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
      "db:test:rm": "docker compose rm test-db -s -f -v",
      "db:test:up": "docker compose up test-db -d",
      "db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 1 && npm run prisma:test:deploy",
      "start:dev": "webpack --config webpack-hmr.config.js --watch",
      "pretest:e2e": "npm run db:test:restart",
      "test:e2e": "dotenv -e .env.test -- jest --watch --no-cache --config ./src/test/jest-e2e.json",
      "format": "eslint --fix . && prettier --write ."
   },
   "dependencies": {
      "@nestjs/common": "^10.2.7",
      "@nestjs/config": "^3.1.1",
      "@nestjs/core": "^10.2.7",
      "@nestjs/jwt": "^10.1.1",
      "@nestjs/passport": "^10.0.2",
      "@nestjs/platform-express": "^10.2.7",
      "@prisma/client": "^5.5.0",
      "argon2": "^0.31.1",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.1"
   },
   "devDependencies": {
      "@nestjs/testing": "^10.2.7",
      "@types/jest": "^29.5.6",
      "@types/mocha": "^10.0.3",
      "@types/passport-jwt": "^3.0.11",
      "@typescript-eslint/eslint-plugin": "^6.9.0",
      "@typescript-eslint/parser": "^6.8.0",
      "dotenv-cli": "^7.3.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-prettier": "^5.0.1",
      "jest": "^29.7.0",
      "mocha": "^10.2.0",
      "pactum": "^3.5.1",
      "prisma": "^5.5.0",
      "run-script-webpack-plugin": "^0.2.0",
      "ts-jest": "^29.1.1",
      "ts-loader": "^9.5.0",
      "ts-node": "^10.9.1",
      "typescript": "^5.2.2",
      "webpack": "^5.89.0",
      "webpack-cli": "^5.1.4",
      "webpack-node-externals": "^3.0.0"
   }
}
